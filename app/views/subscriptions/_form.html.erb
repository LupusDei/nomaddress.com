<%= form_for(@subscribable, :url => {:controller => :subscriptions, :action => :create}) do |f| %>
  <%= f.hidden_field :address_id, :value => @subscribable.address_id %>
  <div class="field">
    <%= f.label :driver_license %><br />
    <%= f.text_field :driver_license %>
  </div>
  <div class="field">
    <%= f.label "Last 4 of SSN" %><br />
    <%= f.text_field :ssn %>
  </div>
  <div class="field">
    <%= f.label :county %><br />
    <%= f.text_field :county %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
